import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    printDecreasing(n);
		
	}
	public static void printDecreasing(int n) {
	    if(n == 1){
	        System.out.println(n);
	        return;
	    }
	    System.out.println(n);
	    printDecreasing(n-1);
	    
	}
}
