import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    
	    int n = sc.nextInt();
	    
	    System.out.println(sumOfIndividualDigits(n));
		
	}
	public static int sumOfIndividualDigits(int n) {
	    if(n == 0) return n;
	    return n%10 + sumOfIndividualDigits(n/10);
	    
	    
	}
}
