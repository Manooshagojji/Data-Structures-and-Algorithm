import java.util.*;
public class Main{
    public static void main(String[] args) {
       Scanner sc  = new Scanner(System.in);
       int n = sc.nextInt();
       int[] arr = new int[n];
       for(int i=0;i<n;i++) {
           arr[i] = sc.nextInt();
       }
       System.out.println(checkArraySorted(arr,n,0));
    }
    public static boolean checkArraySorted(int[] arr,int n,int i) {
        if(i == n-1) return true;
        return arr[i] < arr[i+1] && checkArraySorted(arr,n,i+1);
        
    }

    
}
