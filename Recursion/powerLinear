import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    int x = sc.nextInt();
	    int n = sc.nextInt();
	    System.out.println(powerLog(x,n));
		
	}
	public static int powerLog(int x,int n) {
	    if(n == 0) return 1;
	    
	    
	     int subAns = powerLog(x,n/2);
	     int ans = subAns*subAns;
	     if(n%2 != 0 ) ans *= x;
	    
	     
	         return ans;
	    
	}
}
