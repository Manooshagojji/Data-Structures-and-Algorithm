import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    
	    int n = sc.nextInt();
	    
	    System.out.println(digitProduct(n));
		
	}
	public static int digitProduct(int n) {
	    if(n == 0) return 1;
	    return n%10 * digitProduct(n/10);
	    
	    
	}
}
