// return the list but dont take it in argument, create in bodyy
import java.util.*;
public class Main{
    public static void main(String[] args) {
       Scanner sc  = new Scanner(System.in);
       int n = sc.nextInt();
       int[] arr = new int[n];
       for(int i=0;i<n;i++) {
           arr[i] = sc.nextInt();
       }
       int t = sc.nextInt();
       
       System.out.println(firstandlastOcuuranceOfNumber(arr,0,t));
       //System.out.println(list);
    }
    public static ArrayList<Integer> firstandlastOcuuranceOfNumber(int[] arr,int i,int t) {
        ArrayList<Integer> list = new ArrayList<>();
        if(i == arr.length) {
            return list;
        } 
        if(arr[i] == t) {
            list.add(i);
        }
        
        ArrayList<Integer> ansfromBelowCalls =  firstandlastOcuuranceOfNumber(arr,i+1,t);
        list.addAll(ansfromBelowCalls);
        return list;
        
        
    }

    
}

