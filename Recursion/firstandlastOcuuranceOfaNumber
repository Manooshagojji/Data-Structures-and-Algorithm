// vth return type and list passed in argument
import java.util.*;
public class Main{
    public static void main(String[] args) {
       Scanner sc  = new Scanner(System.in);
       int n = sc.nextInt();
       int[] arr = new int[n];
       for(int i=0;i<n;i++) {
           arr[i] = sc.nextInt();
       }
       int t = sc.nextInt();
       ArrayList<Integer> list = new ArrayList<>();
       System.out.println(firstandlastOcuuranceOfNumber(arr,0,list,t));
       //System.out.println(list);
    }
    public static ArrayList<Integer> firstandlastOcuuranceOfNumber(int[] arr,int i,ArrayList<Integer> list,int t) {
        if(i == arr.length) {
            return list;
        } 
        if(arr[i] == t) {
            list.add(i);
        }
        return firstandlastOcuuranceOfNumber(arr,i+1,list,t);
        
        
    }

    
}
